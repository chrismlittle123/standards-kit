{
  "name": "@standards-kit/conform",
  "version": "0.2.0",
  "description": "Standards conformance checking - in-repo enforcement CLI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "conform": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "linting",
    "code-quality",
    "standards",
    "process",
    "ci",
    "cli"
  ],
  "author": "Chris Little",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrismlittle123/standards-kit.git",
    "directory": "packages/conform"
  },
  "bugs": {
    "url": "https://github.com/chrismlittle123/standards-kit/issues"
  },
  "homepage": "https://github.com/chrismlittle123/standards-kit/tree/main/packages/conform#readme",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.974.0",
    "@aws-sdk/client-dynamodb": "^3.974.0",
    "@aws-sdk/client-ec2": "^3.975.0",
    "@aws-sdk/client-ecs": "^3.975.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.975.0",
    "@aws-sdk/client-elasticache": "^3.975.0",
    "@aws-sdk/client-iam": "^3.974.0",
    "@aws-sdk/client-lambda": "^3.974.0",
    "@aws-sdk/client-rds": "^3.975.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/client-secrets-manager": "^3.974.0",
    "@aws-sdk/client-sns": "^3.974.0",
    "@aws-sdk/client-sqs": "^3.974.0",
    "@google-cloud/artifact-registry": "^4.6.0",
    "@google-cloud/run": "^3.1.0",
    "@google-cloud/secret-manager": "^6.1.1",
    "@iarna/toml": "^2.2.5",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "chalk": "^5.6.2",
    "commander": "^14.0.0",
    "execa": "^9.6.1",
    "glob": "^13.0.0",
    "google-auth-library": "^10.5.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "minimatch": "^10.1.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.9",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.1.8"
  }
}
