{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": [],
      "project": []
    },
    "packages/conform": {
      "entry": ["src/mcp/server.ts"],
      "project": ["src/**/*.ts"],
      "ignore": ["dist/**"]
    },
    "packages/drift": {
      "project": ["src/**/*.ts"],
      "ignore": ["dist/**"]
    }
  },
  "ignoreBinaries": ["ruff", "gitleaks", "uvx", "pip-audit", "vulture", "gh"]
}
